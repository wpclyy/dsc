/*网格布局*/

.dis-box {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}

.flex-box {
  flex: 1;
}

.box-flex {
  display: flex;
  flex-direction: row;
}

/*流式布局*/

.dis-flex {
  width: 100%;
  display: flex;
  flex-flow: wrap;
  align-content: flex-start;
}

.flex-five {
  box-sizing: border-box;
  flex: 0 0 50%;
}

.flex-three {
  box-sizing: border-box;
  flex: 0 0 33.33%;
}

.flex-four {
  box-sizing: border-box;
  flex: 0 0 25%;
}

.flex-one {
  box-sizing: border-box;
  flex: 0 0 10%;
}

/*垂直居中*/

.justify-c {
  display: flex;
  justify-content: center;
}

/*水平居中*/

.align-c {
  align-items: center;
  display: flex;
}

/*居中*/

.text-c {
  text-align: center;
  display: block;
}

.text-r {
  text-align: right;
}

